# Moyasar Configuration (للسعودية)
MOYASAR_API_KEY=
MOYASAR_SECRET_KEY=

# AWS S3
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# Filesystem
FILESYSTEM_DISK=local

# Session & Queue
SESSION_DRIVER=database
QUEUE_CONNECTION=redis

# Cache
CACHE_DRIVER=redis

# Google Analytics
GOOGLE_ANALYTICS_ID=

# Sentry (Error Tracking)
SENTRY_LARAVEL_DSN=
